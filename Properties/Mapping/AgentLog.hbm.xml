<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping auto-import="true" 
                   xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                   xmlns="urn:nhibernate-mapping-2.2"> 

  <class name="Netgao.Telephony.Workflow.Domain.AgentLog, Netgao.Telephony.Workflow" 
         table="Telephony_AgentLogs" lazy="false">

    <id name="Id" access="property" column="ID" type="Int32">
      <generator class="native" />
    </id>

    <many-to-one name="Agent" access="property" class="Netgao.Telephony.Workflow.Domain.Agent, Netgao.Telephony.Workflow" column="AGENT_ID" />

    <property name="Status" access="property">
      <column name="STATUS" />
    </property>

    <property name="XStatus" access="property" type="String">
      <column name="X_STATUS" length="255"  />
    </property>

    <property name="OccurTime" access="property" type="DateTime">
      <column name="OCCUR_TIME"/>
    </property>

  </class>

</hibernate-mapping>
