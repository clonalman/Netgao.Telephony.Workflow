<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping auto-import="true" 
                   xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                   xmlns="urn:nhibernate-mapping-2.2"> 

  <class name="Netgao.Telephony.Workflow.Domain.FilingFolder, Netgao.Telephony.Workflow" 
         table="Telephony_FilingFolders" lazy="false">

    <id name="Id" access="property" column="ID" type="Int32">
      <generator class="native" />
    </id>

    <property name="Name" access="property" type="String">
      <column name="NAME" length="255"  not-null="true" />
    </property>
    
    <property name="Path" access="property" type="String">
      <column name="PATH" length="255"  not-null="true" />
    </property>
    
  </class>

</hibernate-mapping>
